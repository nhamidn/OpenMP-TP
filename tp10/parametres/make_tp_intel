CMD         ?= strassen
SRC         ?= strassen.F90
FFLAGS      += -O1
CFLAGS      += -O1
LDFLAGS     += calcul_erreur_O2.o

first: default

DEPLIB      = calcul_erreur_O2.o

%_O2.o: %.F90
	$(FC) -O2 -c $< -o $@

%_O2.o: %.c
	$(CC) -std=c99 -O2 -c $< -o $@
